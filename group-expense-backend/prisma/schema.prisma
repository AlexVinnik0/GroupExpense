generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Group {
  id        Int      @id @default(autoincrement())
  name      String
  createdBy String
  userId    String   // Required - Supabase user ID
  createdAt DateTime @default(now())
}
